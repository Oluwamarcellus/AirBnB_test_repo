#!/usr/bin/python3
"""
TEST
"""
import unittest
from models.base_model import BaseModel
from models.amenity import Amenity


class TestBaseModel(unittest.TestCase):
    def test_str(self):
        amenity = Amenity()
        self.assertEqual(amenity.name, "")

    def test_p(self):
        amenity = Amenity()
        self.assertTrue(isinstance(amenity, Amenity))
